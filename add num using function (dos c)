#include <stdio.h>
#include <conio.h>

int addfun(int x, int y);   // âœ… semicolon added

int main()
{
    int a, b, result;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    result = addfun(a, b);

    printf("Number is: %d", result);

    getch();
    return 0;
}

// function definition
int addfun(int x, int y)
{
    return x + y;
}
